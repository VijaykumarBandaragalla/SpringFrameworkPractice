			<list>
				<value>BMW</value>
				<value>BMTC</value>
				<value>Maruti800</value>
				<value>bmw</value>
			</list>
		</property>
		<property name="email">
			<set>
				<value>vijay@123</value>
				<value>ajay@187</value>
				<value>suraj@65</value>
			</set>
		</property>
		<property name="name">
			<map>
				<entry key="raj" value="kumar" />
				<entry key="ram" value="manish" />
				<entry key="ras" value="kesh" />
			</map>
		</property>

	</bean>
</beans>








